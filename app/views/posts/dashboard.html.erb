<%= render 'form', tweet: @tweet %>

<table class = "table">
  <thead>
    <tr>
      <th> Title </th>
      <th> Date </th>
      <th> Image Url </th>
      <th> Content </th>
    </tr>
  </thead>
  <tbody>
    <% @posts.reverse.each do |post| %>
    <tr>
    <td><%= post.title %></td>
    <td><%= post.date %></td>
    <td><%= post.image_url %></td>
    <td><%= post.content %></td>
    <tr>
    <% end %>
  </tbody>
</table>